          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
            <stringProp name="scriptLanguage">ecmascript</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">function unspentForAddress(unspent, address, color) {
  return Object.keys(unspent)
    .filter(
      k =&gt;
        unspent[k] &amp;&amp;
        unspent[k].address.toLowerCase() === address.toLowerCase() &amp;&amp;
        (color !== undefined ? unspent[k].color === color : true)
    )
    .map(k =&gt; ({
      outpoint: k,
      output: unspent[k],
    }))
    .sort((a, b) =&gt; {
      return a.output.value - b.output.value; 
    });
};</stringProp>
